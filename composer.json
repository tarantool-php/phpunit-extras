{
    "name": "tarantool/phpunit-extras",
    "description": "A collection of helpers for PHPUnit to ease testing Tarantool libraries.",
    "keywords": ["tarantool", "phpunit", "annotations", "assertions", "expectations", "extensions", "mocking"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Leonovich",
            "email": "gen.work@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8",
        "composer/semver": "^1.5",
        "composer/package-versions-deprecated": "^1.4",
        "rybakit/msgpack": "^0.7|^0.8",
        "rybakit/phpunit-extras": "^0.2",
        "symfony/expression-language": "^3.3|^4|^5",
        "tarantool/client": "^0.8|^0.9"
    },
    "require-dev": {
        "php": "^7.1.3|^8",
        "ext-msgpack": "*",
        "friendsofphp/php-cs-fixer": "^2.14",
        "vimeo/psalm": "^3.9|^4"
    },
    "autoload": {
        "psr-4": {
            "Tarantool\\PhpUnit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tarantool\\PhpUnit\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
